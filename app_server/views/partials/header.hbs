<div id="header">
    <div id="logo">
        <a href="/"><img src="/images/logo.png" alt="LOGO" height="100" width="200"></a>
    </div>
    <div id="navigation">
        <ul style="display: flex; align-items: center;">
            <li class="{{#if (eq current 'home')}}selected{{/if}}">
                <a href="/">Home</a>
            </li>
            <li class="{{#if (eq current 'travel')}}selected{{/if}}">
                <a href="/travel">Travel</a>
            </li>
            <li class="{{#if (eq current 'rooms')}}selected{{/if}}">
                <a href="/rooms">Rooms</a>
            </li>
            <li class="{{#if (eq current 'meals')}}selected{{/if}}">
                <a href="/meals">Meals</a>
            </li>
            <li class="{{#if (eq current 'news')}}selected{{/if}}">
                <a href="/news">News</a>
            </li>
            <li class="{{#if (eq current 'about')}}selected{{/if}}">
                <a href="/about">About</a>
            </li>
            <li class="{{#if (eq current 'contact')}}selected{{/if}}">
                <a href="/contact">Contact</a>
            </li>
            <li style="margin-left: auto;" class="{{#if (eq current 'login')}}selected{{/if}}">
                {{#if isAuthenticated}}
                    <a href="/logout">Logout</a>
                {{else}}
                    <a href="/login">Login</a>
                {{/if}}
            </li>
        </ul>
    </div>
    <!-- Logout button -->
    <div id="footer">
        <button onclick="window.location.href='/logout'">Logout</button>
    </div>

</div>

<p>isAuthenticated: {{isAuthenticated}}</p> <!-- Line for for debugging -->

